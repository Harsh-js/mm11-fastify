{
	"name": "usinuer_backend",
	"version": "3.0.0",
	"description": "",
	"main": "index.js",
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "node -r tsconfig-paths/register -r ts-node/register",
		"ext": "ts, js"
	},
	"scripts": {
		"test": "tsnd --respawn --transpile-only --exit-child --require tsconfig-paths/register src/test/index.ts",
		"watch": "tsc-alias -w & tsc -w",
		"build": "tsc && tsc-alias",
		"socket": "npm run dev src/socket/app.ts",
		"app": "tsnd --respawn --transpile-only --exit-child --require tsconfig-paths/register src/client/app.ts",
		"cron": "nodemon src/socket/cron.ts",
		"dev": "tsnd --respawn --transpile-only --exit-child --require tsconfig-paths/register",
		"push": "tsnd --respawn --transpile-only --exit-child --require tsconfig-paths/register src/git/push.ts",
		"start": "node -r tsconfig-paths/register -r ts-node/register",
		"migrate": "npm start src/db/migrate.ts && npm start src/db/jsonSchema.ts",
		"g:module": "npm run dev src/dev/utils/generateModule.ts"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/chalk-animation": "^1.6.1",
		"@types/cors": "^2.8.12",
		"@types/express-fileupload": "^1.4.1",
		"@types/figlet": "^1.5.4",
		"@types/http-errors": "^1.8.1",
		"@types/json-schema": "^7.0.11",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.191",
		"@types/mongoose-sequence": "^3.0.7",
		"@types/morgan": "^1.9.3",
		"@types/response-time": "^2.3.5",
		"nodemon": "^2.0.18",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.8.1",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.4.4"
	},
